#include <stdio.h>

int main() {
//Write a C program to count occurrences of a character in given string.
char str1[50],key;
int i=0,count=0;
printf("Enter any string: ");
gets(str1);
printf("Enter a character: ");
scanf("%c",&key);
while(str1[i]!='\0') {
    if (str1[i]==key)
    count++;
    i++;

}
printf("%c is found %d times",key,count);

return 0;
}
